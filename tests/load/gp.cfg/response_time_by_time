# input variables
#
#   input_filename
#   title
#   output_filename

set terminal pngcairo size 3200,1800 font "Ubuntu,36"
set size 1, 1
set output output_filename
set title title font "Ubuntu Bold,36"
set key left top
set grid y

set timefmt "%s"

set xdata time
set xlabel 'Time in Test (minute:second)'

set ylabel "Response Time (milliseconds)"

set datafile separator '\t'

plot input_filename every ::2 using 2:5 title "" with points

exit
