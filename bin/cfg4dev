if [ -f "$PWD/.801dbe4659a641739cbe94fcf0baab03" ]; then
	if [ -d "$PWD/env" ]; then
		source "$PWD/env/bin/activate"
	else
		virtualenv env
		source "$PWD/env/bin/activate"
		pip install --requirement "$PWD/requirements.txt"
		git clone https://github.com/dpnova/tornado-memcache.git
	fi
	export PATH=$PATH:"$PWD/bin"
	export PYTHONPATH="$PWD/lib:$PWD/tornado-memcache"
else
	echo "Must source this script from root dir of apimgmt project"
fi
